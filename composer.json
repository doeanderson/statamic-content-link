{
    "name": "doeanderson/statamic-content-link",
    "description": "Symlink Statamic content from an external location",
    "keywords": [
        "Doe-Anderson",
        "laravel",
        "statamic-content-link"
    ],
    "homepage": "https://github.com/doeanderson/statamic-content-link",
    "license": "MIT",
    "authors": [
        {
            "name": "Kenton Glass",
            "email": "kglass@doeanderson.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "spatie/laravel-package-tools": "^1.14.0",
        "illuminate/contracts": "^9.0|^10.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "DoeAnderson\\StatamicContentLink\\": "src/"
        }
    },
    "scripts": {
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "DoeAnderson\\StatamicContentLink\\StatamicContentLinkServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
